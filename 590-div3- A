#include<bits/stdc++.h>
using namespace std;
 
int main()
{
    int t,a;
    cin>>t;
    while(t--)
    {
        int n,i;
        cin>>n;
        int sum=0;
        for(i=0; i<n; i++)
        {
            cin>>a;
            sum = sum+a;
        }
        if(sum%n>0)
            cout<<sum/n+1<<endl;
        else
            cout<<sum/n<<endl;
    }
}
